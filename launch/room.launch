<?xml version="1.0" ?>
<launch>
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <!-- more default parameters can be changed here -->
  </include>
  <!-- Spawn a room into Gazebo -->
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-file $(find virtual_scan)/models/room.sdf -sdf -model room" />
  <!-- Spawn a robot into Gazebo -->
  <node name="spawn_urdf0" pkg="gazebo_ros" type="spawn_model" args="-file $(find virtual_scan)/srbot_description/srbot0.urdf -urdf -y -1 -z 1 -model test0" />
  <node name="spawn_urdf1" pkg="gazebo_ros" type="spawn_model" args="-file $(find virtual_scan)/srbot_description/srbot1.urdf -urdf -y 0 -z 1 -model test1" />
  <node name="spawn_urdf2" pkg="gazebo_ros" type="spawn_model" args="-file $(find virtual_scan)/srbot_description/srbot2.urdf -urdf -y 1 -z 1 -model test2" />
</launch>
